{
  "name": "frontend",
  "version": "1.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "test:coverage": "vitest run --coverage",
    "test:ci": "NODE_OPTIONS='--trace-deprecation' vitest run --coverage",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "lint:oxlint": "oxlint . --fix -D correctness --ignore-path .gitignore",
    "lint:eslint": "eslint . --fix",
    "lint": "run-s lint:*",
    "prettier:fix": "prettier '**/*.*' --write",
    "prettier:check": "prettier . --check",
    "ci": "run-s lint prettier:check type-check test:ci"
  },
  "dependencies": {
    "@fontsource/inter": "^5.2.8",
    "@primeuix/themes": "^1.2.5",
    "@supabase/supabase-js": "^2.87.1",
    "@tailwindcss/vite": "^4.1.17",
    "@tanstack/vue-query": "^5.92.1",
    "@tanstack/vue-query-devtools": "^6.1.2",
    "axios": "^1.13.2",
    "mitt": "^3.0.1",
    "primeicons": "^7.0.0",
    "primevue": "^4.5.2",
    "tailwindcss": "^4.1.17",
    "tailwindcss-primeui": "^0.6.1",
    "vue": "^3.5.25",
    "vue-router": "^4.6.3"
  },
  "devDependencies": {
    "@faker-js/faker": "^10.1.0",
    "@tsconfig/node24": "^24.0.3",
    "@types/jsdom": "^27.0.0",
    "@types/lodash": "^4.17.21",
    "@types/node": "^24.10.2",
    "@vitejs/plugin-vue": "^6.0.2",
    "@vitejs/plugin-vue-jsx": "^5.1.2",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/eslint-plugin": "^1.5.2",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.5.1",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.8.1",
    "eslint": "^9.39.1",
    "eslint-plugin-oxlint": "~1.31.0",
    "eslint-plugin-vue": "~10.6.2",
    "jiti": "^2.6.1",
    "jsdom": "^27.2.0",
    "lodash": "^4.17.21",
    "msw": "^2.12.4",
    "npm-run-all2": "^8.0.4",
    "oxlint": "~1.31.0",
    "prettier": "3.7.3",
    "type-fest": "^5.3.1",
    "typescript": "~5.8.3",
    "vite": "^7.2.7",
    "vite-plugin-vue-devtools": "^8.0.5",
    "vitest": "^3.2.4",
    "vue-router-mock": "^2.0.0",
    "vue-tsc": "^3.1.8"
  }
}
